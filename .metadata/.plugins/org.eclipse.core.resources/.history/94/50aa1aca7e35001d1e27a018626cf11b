package com.rk.dto;

import org.springframework.data.annotation.Id;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

/**
 * @author RK
 *
 */

@Data
@NoArgsConstructor
@AllArgsConstructor
public class Category {
	
	@Id
	private Integer id;
	private String name;
	private String brand;
	
	
}
